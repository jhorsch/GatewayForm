<h1>Login</h1>


 <%= form_for(:session, url: sessions_path ) do |f| %>



      <p><%= f.label :email %>
      <%= f.text_field :email %></p>

      <p><%= f.label :password %>
      <%= f.password_field :password %></p>


      <p><%= f.submit "Log in", class: "btn btn-large btn-primary" %></p>
    <% end %>

    <p> New To Instagram? <%= link_to 'Sign Up', 'signup' %></p>
